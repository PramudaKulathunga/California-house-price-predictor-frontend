{
  "name": "california_house_price_predictor",
  "version": "1.0.0",
  "description": "React frontend for California Housing Price Prediction",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "axios": "^1.12.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-scripts": "5.0.1",
    "schema-utils": "^4.3.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    
    "deploy": "npm run build && vercel --prod",
    "deploy:dev": "vercel",
    "deploy:with-env": "vercel --env REACT_APP_API_URL=$REACT_APP_API_URL --env REACT_APP_ENVIRONMENT=production",
    
    "verify:build": "npm run build && npx serve -s build -p 3001",
    "verify:env": "node -e \"console.log('API URL:', process.env.REACT_APP_API_URL || 'Not set'); console.log('Node env:', process.env.NODE_ENV)\"",
    "setup:vercel": "npm install -g vercel",
    "setup:env": "cp .env.example .env.local",

    "logs": "vercel logs",
    "open": "vercel open"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}